{
  "name": "NodeJS-API",
  "version": 2,
  "public": true,
  "alias": [
    "nodejs-api.danetheory.com"
  ],
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false,
    "autoJobCancelation": true
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
     }
  ],
  "routes": [
    {
      "src": "/(.*)",
       "dest": "/index.js"
    }
  ]
}
